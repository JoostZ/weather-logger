<Window x:Class="WeatherLogger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="354" Width="873" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Height="100" HorizontalAlignment="Left" Name="stackPanel1" VerticalAlignment="Top" Orientation="Horizontal">
            <TextBox Height="23" Name="textBox1" Width="120" />
            <Button Content="Button" Height="23" Name="button1" Width="75" />
            <DatePicker Height="25" Name="datePicker1" Width="115" />
            <DatePicker Height="25" Name="datePicker2" Width="115" />
            <Button Content="Button" Height="23" Name="button2" Width="75" />
            <TextBox Height="23" Name="textBox2" Width="120" IsReadOnly="True" />
        </StackPanel>
    </Grid>
</Window>
